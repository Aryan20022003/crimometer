{
    "compilerOptions": {
      "jsx": "react",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strictNullChecks": true,
      "module": "esnext",
      "target": "es5",
      "lib": ["dom", "dom.iterable", "esnext"],
      "sourceMap": true,
      "outDir": "./dist",
      "moduleResolution": "node",
      "resolveJsonModule": true
    },
    "include": ["src/**/*","src/Report/*"]
  }
  